{
  "name": "typesense-migrations",
  "version": "0.0.1",
  "description": "Migrations manager for Typesense",
  "repository": {
    "type": "git",
    "url": "git://github.com/TiagoDamascena/typesense-migrations.git"
  },
  "keywords": [
    "typesense",
    "migrations"
  ],
  "author": "Tiago Damascena",
  "license": "MIT",
  "bin": {
    "typesense-migrations": "./dist/index.js"
  },
  "scripts": {
    "build": "npx tsc && copyfiles -f src/templates/* dist/templates",
    "watch": "npx tsc -w"
  },
  "dependencies": {
    "typesense": "^1.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/yargs": "^17.0.33",
    "copyfiles": "^2.4.1",
    "typescript": "^5.5.4"
  }
}
